{
  "name": "wdlang",
  "author": "",
  "displayName": "wdlang",
  "description": "framewotk for tkinter ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "wdlang",
        "aliases": [
          "wdlang",
          "wdlang"
        ],
        "extensions": [
          ".wd"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "wdlang",
        "scopeName": "source.wd",
        "path": "./syntaxes/wdlang.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "wdlang",
        "title": "wdlang: New Project"
      }
    ],
    "semanticTokenScopes": [
      {
        "language": "wdlang",
        "scopes": {
          "annotation": [
            "variable"
          ],
          "keyword": [
            "keyword"
          ],
          "keyword.control": [
            "keyword.control"
          ],
          "string.escape": [
            "constant.character.escape"
          ],
          "source": [
            "meta.embedded"
          ]
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "comp_yaml": "npx js-yaml syntaxes/wdlang.tmLanguage.yaml > syntaxes/wdlang.tmLanguage.json"
  },
  
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "js-yaml": "^4.1.0",
    "typescript": "^4.7.2"
  },

  "dependencies": {
    "tslint": "^6.1.3"
  },

  "main": "./out/extension.js",

  "activationEvents": [
    "onLanguage:wdlang"
  ]
}
